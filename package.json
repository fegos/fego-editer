{
  "name": "nsip-editor",
  "version": "1.0.0",
  "description": "基于draftJS的富文本编辑器",
  "main": "dist/index.js",
  "scripts": {
    "init": "npm install --no-options -s --registry=https://registry.npm.taobao.org && npm run init-end",
    "init-end": "rimraf package-lock.json",
    "build": "npm run clean && webpack --progress --profile --colors --config webpack.config.js",
    "clean": "rimraf -rf ./dist",
    "parcel": "parcel ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.ms.netease.com/bjfuxin1/nsip-editor.git"
  },
  "keywords": [
    "draftjs",
    "editor"
  ],
  "author": "fx",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "draft-js": "^0.10.4",
    "draftjs-utils": "^0.9.0",
    "immutable": "^3.8.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-react-hmre": "^1.1.1"
  }
}
